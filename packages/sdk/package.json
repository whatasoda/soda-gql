{
  "name": "@soda-gql/sdk",
  "version": "0.11.9",
  "description": "Programmatic SDK for soda-gql CLI features",
  "type": "module",
  "private": false,
  "license": "MIT",
  "files": [
    "dist",
    "index.d.ts",
    "index.js"
  ],
  "author": {
    "name": "Shota Hatada",
    "email": "shota.hatada@whatasoda.me",
    "url": "https://github.com/whatasoda"
  },
  "keywords": [
    "graphql",
    "codegen",
    "zero-runtime",
    "typescript",
    "sdk",
    "programmatic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/whatasoda/soda-gql.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/whatasoda/soda-gql#readme",
  "bugs": {
    "url": "https://github.com/whatasoda/soda-gql/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "@soda-gql": "./@x-index.ts",
