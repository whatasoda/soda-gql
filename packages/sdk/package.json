      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@soda-gql/builder": "workspace:*",
    "@soda-gql/codegen": "workspace:*",
    "@soda-gql/config": "workspace:*",
    "@soda-gql/core": "workspace:*",
    "neverthrow": "^8.1.1"
  }
