{
  "name": "@soda-gql/plugin-nestjs",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts",
      "default": "./src/types.ts"
    },
    "./errors": {
      "types": "./src/errors.ts",
      "import": "./src/errors.ts",
      "default": "./src/errors.ts"
    },
    "./webpack": {
      "types": "./src/webpack/index.ts",
      "import": "./src/webpack/index.ts",
      "default": "./src/webpack/index.ts"
    },
    "./webpack/plugin": {
      "types": "./src/webpack/plugin.ts",
      "import": "./src/webpack/plugin.ts",
      "default": "./src/webpack/plugin.ts"
    },
    "./webpack/loader": {
      "types": "./src/webpack/loader.ts",
      "import": "./src/webpack/loader.ts",
      "default": "./src/webpack/loader.ts"
    },
    "./config": {
      "types": "./src/config/index.ts",
      "import": "./src/config/index.ts",
      "default": "./src/config/index.ts"
    },
    "./package.json": "./package.json",
    "./internal/*": null
  },
  "dependencies": {
    "@soda-gql/builder": "workspace:*",
    "@soda-gql/common": "workspace:*",
    "@soda-gql/core": "workspace:*",
    "@soda-gql/plugin-babel": "workspace:*",
    "@soda-gql/plugin-shared": "workspace:*",
    "@soda-gql/runtime": "workspace:*",
    "neverthrow": "^8.1.1",
    "zod": "^4.1.5"
  },
  "peerDependencies": {
    "@babel/core": "^7.24.0",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@types/webpack": "^5.28.5"
  }
}
