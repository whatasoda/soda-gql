{
  "name": "@soda-gql/plugin-nestjs",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./webpack": {
      "development": "./src/webpack/index.ts",
      "types": "./dist/webpack/index.d.ts",
      "import": "./dist/webpack/index.js",
      "require": "./dist/webpack/index.cjs",
      "default": "./dist/webpack/index.js"
    },
    "./webpack/plugin": {
      "development": "./src/webpack/plugin.ts",
      "types": "./dist/webpack/plugin.d.ts",
      "import": "./dist/webpack/plugin.js",
      "require": "./dist/webpack/plugin.cjs",
      "default": "./dist/webpack/plugin.js"
    },
    "./webpack/loader": {
      "development": "./src/webpack/loader.ts",
      "types": "./dist/webpack/loader.d.ts",
      "import": "./dist/webpack/loader.js",
      "require": "./dist/webpack/loader.cjs",
      "default": "./dist/webpack/loader.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@soda-gql/builder": "workspace:*",
    "@soda-gql/common": "workspace:*",
    "@soda-gql/core": "workspace:*",
    "@soda-gql/plugin-shared": "workspace:*",
    "@soda-gql/plugin-webpack": "workspace:*",
    "@soda-gql/runtime": "workspace:*",
    "neverthrow": "^8.1.1",
    "zod": "^4.1.11"
  },
  "peerDependencies": {
    "@babel/core": "^7.24.0",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@types/webpack": "^5.28.5"
  }
}
