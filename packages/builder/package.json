{
  "name": "@soda-gql/builder",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts",
      "default": "./src/types.ts"
    },
    "./change-set": {
      "types": "./src/change-set.ts",
      "import": "./src/change-set.ts",
      "default": "./src/change-set.ts"
    },
    "./artifact": {
      "types": "./src/artifact/index.ts",
      "import": "./src/artifact/index.ts",
      "default": "./src/artifact/index.ts"
    },
    "./schemas/artifact": {
      "types": "./src/schemas/artifact.ts",
      "import": "./src/schemas/artifact.ts",
      "default": "./src/schemas/artifact.ts"
    },
    "./service": {
      "types": "./src/service.ts",
      "import": "./src/service.ts",
      "default": "./src/service.ts"
    },
    "./package.json": "./package.json",
    "./internal/*": null
  },
  "dependencies": {
    "@soda-gql/codegen": "workspace:*",
    "@soda-gql/common": "workspace:*",
    "@soda-gql/core": "workspace:*",
    "@swc/core": "^1.6.3",
    "@swc/types": "^0.1.6",
    "neverthrow": "^8.1.1",
    "typescript": "^5.0.0",
    "zod": "^4.1.11"
  }
}
