{
  "name": "@soda-gql/builder",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./types": {
      "development": "./src/types.ts",
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs",
      "default": "./dist/types.js"
    },
    "./change-set": {
      "development": "./src/change-set.ts",
      "types": "./dist/change-set.d.ts",
      "import": "./dist/change-set.js",
      "require": "./dist/change-set.cjs",
      "default": "./dist/change-set.js"
    },
    "./artifact": {
      "development": "./src/artifact/index.ts",
      "types": "./dist/artifact/index.d.ts",
      "import": "./dist/artifact/index.js",
      "require": "./dist/artifact/index.cjs",
      "default": "./dist/artifact/index.js"
    },
    "./schemas/artifact": {
      "development": "./src/schemas/artifact.ts",
      "types": "./dist/schemas/artifact.d.ts",
      "import": "./dist/schemas/artifact.js",
      "require": "./dist/schemas/artifact.cjs",
      "default": "./dist/schemas/artifact.js"
    },
    "./service": {
      "development": "./src/service.ts",
      "types": "./dist/service.d.ts",
      "import": "./dist/service.js",
      "require": "./dist/service.cjs",
      "default": "./dist/service.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@soda-gql/codegen": "workspace:*",
    "@soda-gql/common": "workspace:*",
    "@soda-gql/core": "workspace:*",
    "@swc/core": "^1.6.3",
    "@swc/types": "^0.1.6",
    "fast-glob": "^3.3.3",
    "neverthrow": "^8.1.1",
    "typescript": "^5.0.0",
    "xxhash-wasm": "^1.1.0",
    "zod": "^4.1.11"
  }
}
