{
  "name": "@soda-gql/plugin-shared",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./errors": {
      "types": "./src/errors.ts",
      "import": "./src/errors.ts",
      "default": "./src/errors.ts"
    },
    "./transform": {
      "types": "./src/transform/index.ts",
