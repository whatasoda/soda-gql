{
  "name": "@soda-gql/core",
  "version": "0.11.9",
  "description": "Core types and utilities for zero-runtime GraphQL query generation",
  "type": "module",
  "private": false,
  "license": "MIT",
  "files": [
    "dist",
    "_internal.d.ts",
    "_internal.js",
    "adapter.d.ts",
    "adapter.js",
    "index.d.ts",
    "index.js",
    "runtime.d.ts",
    "runtime.js"
  ],
  "author": {
    "name": "Shota Hatada",
    "email": "shota.hatada@whatasoda.me",
    "url": "https://github.com/whatasoda"
  },
  "keywords": [
    "graphql",
    "codegen",
    "zero-runtime",
    "typescript",
    "type-safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/whatasoda/soda-gql.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/whatasoda/soda-gql#readme",
  "bugs": {
    "url": "https://github.com/whatasoda/soda-gql/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "@soda-gql": "./@x-index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./runtime": {
      "@soda-gql": "./@x-runtime.ts",
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.cjs",
      "default": "./dist/runtime.js"
    },
    "./adapter": {
      "@soda-gql": "./@x-adapter.ts",
      "types": "./dist/adapter.d.ts",
      "import": "./dist/adapter.js",
      "require": "./dist/adapter.cjs",
      "default": "./dist/adapter.js"
    },
    "./_internal": {
      "@soda-gql": "./@x-_internal.ts",
      "types": "./dist/_internal.d.ts",
      "import": "./dist/_internal.js",
      "require": "./dist/_internal.cjs",
      "default": "./dist/_internal.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "graphql": "^16.11.0",
    "@graphql-typed-document-node/core": "^3.2.0"
  }
}
